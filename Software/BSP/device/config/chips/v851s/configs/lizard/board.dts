		allwinner,muxsel = <6>;
		allwinner,drive = <0>;
		allwinner,pull = <1>;
	};

	twi3_pins_b: twi3@1 {
		allwinner,pins = "PI3", "PI4";
		allwinner,function = "io_disabled";
		allwinner,muxsel = <0xf>;
		allwinner,drive = <0>;
		allwinner,pull = <0>;
