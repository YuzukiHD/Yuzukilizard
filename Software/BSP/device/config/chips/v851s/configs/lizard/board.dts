				 "PE3", "PE4", "PE5";
		allwinner,function = "io_disabled";
		allwinner,muxsel = <7>;
		allwinner,drive = <1>;
		allwinner,pull = <1>;
	};

	dsi2lane_pins_a: dsi2lane@0 {
