	daudio1_pins_b: daudio1_sleep@0 {
		allwinner,pins = "PE7", "PE8", "PE9", "PE10", "PE11";
		allwinner,function = "io_disabled";
		allwinner,muxsel = <0xf>;
		allwinner,drive = <1>;
		allwinner,pull = <0>;
	};

	pwm6_pin_a: pwm6@0 {
