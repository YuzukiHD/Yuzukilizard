				 "PE3", "PE4", "PE5";
		allwinner,function = "sdc1";
		allwinner,muxsel = <6>;
		allwinner,drive = <3>;
		allwinner,pull = <1>;
	};

	sdc1_pins_b: sdc1@1 {
